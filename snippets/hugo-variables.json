{
  "hugo-description": {
    "prefix": "h-description",
    "body": ["{{ .Description }}"],
    "description": "Description page variable"
  },
  "hugo-lastmod": {
    "prefix": "h-lastmod",
    "body": ["{{ .Lastmod }}"],
    "description": "Date the content was last modified"
  },
  "hugo-params": {
    "prefix": "h-params",
    "body": ["{{ .Params.${1:date} }}"],
    "description": "Params variable"
  },
  "hugo-publish-date": {
    "prefix": "h-publish-date",
    "body": ["{{ .PublishDate }}"],
    "description": "Date on which the content was or will be published"
  },
  "hugo-reading-time": {
    "prefix": "h-reading-time",
    "body": ["{{ .ReadingTime }}"],
    "description": "Estimated time, in minutes, it takes to read the content"
  },
  "hugo-summary": {
    "prefix": "h-summary",
    "body": ["{{ .Summary }}"],
    "description": "Summary page variable"
  },
  "hugo-table-of-contents": {
    "prefix": "h-table-of-contents",
    "body": ["{{ .TableOfContents }}"],
    "description": "TableOfContents page variable"
  },
  "hugo-title": {
    "prefix": "h-title",
    "body": ["{{ .Title }}"],
    "description": "Title page variable"
  },
  "hugo-word-count": {
    "prefix": "h-word-count",
    "body": ["{{ .WordCount }}"],
    "description": "Number of words in the content"
  }
}
